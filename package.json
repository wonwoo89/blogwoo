{
  "name": "blogwoo",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/wonwoo89/blogwoo",
  "author": "Isaac Lee",
  "scripts": {
    "dev": "yarn kill | yarn ssl-proxy | next dev -p 8900",
    "ssl-proxy": "npx local-ssl-proxy --key certificates/local.blogwoo.com-key.pem --cert certificates/local.blogwoo.com.pem --source 8443 --target 8900",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "kill": "lsof -i :8900 | grep LISTEN | awk '{print $2}' | xargs  kill -9"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6"
  }
}
